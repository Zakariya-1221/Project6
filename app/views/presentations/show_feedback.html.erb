<h1>Feedback for <%= @presentation.title %></h1>
<p><strong>Average Rating:</strong> <%= @average_rating %></p>
<p><strong>Final Score:</strong> <%= @final_score %></p>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Rating</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.user.name %></td>
        <td><%= feedback.rating %></td>
        <td><%= feedback.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>