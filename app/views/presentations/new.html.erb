<h1>New Presentation</h1>
<%= form_with model: @presentation, local: true do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>
  <br><br>
  <%= form.label :description %>
  <%= form.text_area :description %>
  <br><br>
  <%= form.label :date%>
  <%= form.date_field :date %>
  <br><br>
  <%= form.submit "Create Presentation" %>
  <!-- clear form -->
  <%= form.submit "Clear", type: "reset"%>
<% end %>